                                                 .Refresh(true)
                                                 .PageSizes(true)
                                                 .ButtonCount(5))
                                           .Filterable()
                                           .Scrollable(h => h.Height(500))
                      .DataSource(dataSource => dataSource
                    .Ajax()
                    .Sort(sort=>sort.Add(s=>s.IdleTimeID).Descending())
                    .Model(model => model.Id(p => p.IdleTimeID))
                    .Read(read => read.Action("IdleTimes_Read", "IdleTime"))
                    .Create(create => create.Action("IdleTimes_Create", "IdleTime"))
                    .Update(update => update.Action("IdleTimes_Update", "IdleTime"))
                    .Destroy(destroy => destroy.Action("IdleTimes_Destroy", "IdleTime"))
